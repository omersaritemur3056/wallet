        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
        <org.projectlombok.lombok>1.18.22</org.projectlombok.lombok>
        <com.fasterxml.jackson.core.jackson-databind>2.14.1</com.fasterxml.jackson.core.jackson-databind>
        <org.junit.jupiter.junit-jupiter-api>5.9.1</org.junit.jupiter.junit-jupiter-api>
        <org.springdoc.openapi>1.6.13</org.springdoc.openapi>
        <org.apache.commons.commons-text>1.10.0</org.apache.commons.commons-text>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.projectlombok</groupId>
                <artifactId>lombok</artifactId>
                <version>${org.projectlombok.lombok}</version>
            </dependency>
            <dependency>
                <groupId>com.fasterxml.jackson.core</groupId>
                <artifactId>jackson-databind</artifactId>
                <version>${com.fasterxml.jackson.core.jackson-databind}</version>
            </dependency>
            <dependency>
                <groupId>org.springdoc</groupId>
                <artifactId>springdoc-openapi-ui</artifactId>
